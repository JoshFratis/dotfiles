source ~/.vimrc

" https://github.com/VsVim/VsVim/wiki/VsVim-Nonstandard-Behavior

" Rename
nnoremap <leader>r :vsc Refactor.Rename<CR>

" Navigate
"map gd :vsc Edit.GoToDefinition<CR><C-c>
map gi :vsc Edit.GoToImplementation<CR>
map gr :vsc Edit.FindAllReferences<CR>
map gp :vsc Edit.PeekDefinition<CR>

" Fix Enter Visual Mode on Go to Definition
nnoremap gd :vsc Edit.GoToDefinition<CR><Esc>

" Test
map <leader>t Atest-one
